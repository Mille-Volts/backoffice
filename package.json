{
  "name": "@millevolts/backoffice",
  "version": "0.3.2",
  "description": "",
  "main": "lib/backoffice.common.js",
  "module": "src/index.js",
  "scripts": {
    "serve": "vue-cli-service serve example/main.js",
    "predeploy": "BUILD_TARGET=app vue-cli-service build --dest dist example/main.js",
    "deploy": "firebase deploy",
    "build": "BUILD_TARGET=lib vue-cli-service build --dest lib --target lib --formats commonjs --name backoffice src/index.js",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "vue": "vue",
    "prepublish": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ]
  },
  "files": [
    "lib/*",
    "src/*",
    "example/*",
    "*.json",
    "*.js"
  ],
  "author": "Mille Volts - https://www.millevolts.fr",
  "license": "ISC",
  "peerDependencies": {
    "element-ui": "^2.6.0"
  },
  "dependencies": {
    "element-ui": "^2.6.1",
    "moment": "^2.24.0",
    "vue": "^2.6.9",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.14.0",
    "vue-sticky-directive": "0.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli": "^3.5.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/cli-service-global": "^3.5.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "core-js": "^2.6.5",
    "eslint": "^5.15.2",
    "eslint-plugin-vue": "^5.2.2",
    "fibers": "^3.1.1",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.9",
    "webpack-node-externals": "^1.7.2"
  }
}
