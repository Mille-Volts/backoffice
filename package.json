{
  "name": "@millevolts/backoffice",
  "version": "0.13.0",
  "description": "",
  "author": "Mille Volts - https://www.millevolts.fr",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "serve": "vue-cli-service serve example/main.js",
    "build": "BUILD_TARGET=lib vue-cli-service build --dest lib --target lib --formats commonjs --name backoffice src/index.js",
    "lint": "vue-cli-service lint",
    "deploy": "firebase deploy",
    "dev": "npm run serve",
    "postversion": "git push && git push --tags",
    "predeploy": "BUILD_TARGET=app vue-cli-service build --dest dist example/main.js",
    "prepublish": "npm run lint && npm run build",
    "preversion": "npm run lint",
    "vue": "vue"
  },
  "main": "lib/backoffice.common.js",
  "module": "src/index.js",
  "files": [
    "lib/*",
    "src/*",
    "example/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "moment": "^2.29.2",
    "vue": "^2.6.12",
    "vue-router": "^3.1.6",
    "vue-scrollto": "^2.17.1",
    "vue-sticky-directive": "0.0.10",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli": "^4.3.1",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/cli-service-global": "^4.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^4.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "peerDependencies": {
    "element-ui": "^2.13.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ]
  },
  "license": "ISC"
}
